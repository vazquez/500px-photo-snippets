<!doctype html>
<html lang="en">
<head>
  <script type="text/javascript" src="//use.typekit.net/fus0vec.js"></script>
  <script type="text/javascript">try{Typekit.load();}catch(e){}</script>
  <meta charset="UTF-8">
  <title>Photo Snippets</title>
</head>
<body>
  <div class="bg"></div>
  
  <div class="wrapper">
  
    <form class="login" method="POST">
    
      <h2>Login with your 500px account...</h2>
    
      <div>
        <label for="user" style="padding: 0 128px 0 0">Username</label>
        <input id="user" name="username" type="textfield" placeholder="username">
      </div>
      
      <div>
        <label for="password">Password</label>
        <input id="password" name="password" type="password" placeholder="password">
      </div>
      
      <div class="submit">
        <p>Forgot your username or password?</p>
        <button type="submit">Share Your Snippet</button>
        
      </div>
    
    </form>
    
  </div>

</body>
<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
